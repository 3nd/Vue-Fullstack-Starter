{
  "name": "Vue-Fullstack-Starter",
  "version": "1.0.0",
  "description": "A boilerplate for Vue, Material, Express, Babel, Flow, and ReactiveX.",
  "scripts": {
    "start:app": "webpack-dev-server --progress",
    "start:api": "nodemon src/server.js --exec babel-node",
    "build:app": "webpack --env.prod",
    "build:api": "babel src -d dist --ignore client.js,app",
    "test:app": "eslint src/app && jest \"src/app/.*\\.js$\" --coverage --forceExit",
    "test:api": "eslint src/api && jest \"src/api/.*\\.js$\" --coverage --forceExit",
    "deploy": "firebase deploy",
    "lint:vue": "eslint src/app/**/*.vue",
    "flow": "flow",
    "typed": "flow-typed install",
    "codecov": "codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shyam-Chen/Vue-Fullstack-Starter.git"
  },
  "keywords": [
    "Vue"
  ],
  "author": "Yen-cheng Chen, 陳彥澄",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shyam-Chen/Vue-Fullstack-Starter/issues"
  },
  "homepage": "https://github.com/Shyam-Chen/Vue-Fullstack-Starter#readme",
  "dependencies": {
    "apollo-cache-inmemory": "1.1.9",
    "apollo-client": "2.2.5",
    "apollo-link": "1.2.1",
    "apollo-link-http": "1.5.1",
    "axios": "0.18.0",
    "cors": "2.8.4",
    "express": "4.16.2",
    "express-history-api-fallback": "2.2.1",
    "graphql": "0.13.1",
    "graphql-tag": "2.8.0",
    "rxjs": "5.5.6",
    "vue": "2.5.13",
    "vue-apollo": "3.0.0-beta.4",
    "vue-i18n": "7.4.2",
    "vue-meta": "1.4.3",
    "vue-router": "3.0.1",
    "vue-rx": "5.0.0",
    "vue-server-renderer": "2.5.13",
    "vuetify": "1.0.3",
    "vuex": "3.0.1",
    "vuex-bound": "0.1.0",
    "vuex-router-sync": "5.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.1",
    "babel-helper-vue-jsx-merge-props": "2.0.3",
    "babel-jest": "22.2.2",
    "babel-loader": "7.1.2",
    "babel-plugin-root-import": "5.1.0",
    "babel-plugin-syntax-jsx": "6.18.0",
    "babel-plugin-transform-function-bind": "6.22.0",
    "babel-plugin-transform-imports": "1.4.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-plugin-transform-vue-jsx": "3.5.1",
    "babel-preset-env": "1.6.1",
    "babel-register": "6.26.0",
    "codecov": "3.0.0",
    "copy-webpack-plugin": "4.4.1",
    "css-loader": "0.28.9",
    "cssnano": "4.0.0-rc.2",
    "eslint": "4.18.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-vue": "4.3.0",
    "firebase-tools": "3.17.4",
    "flow-bin": "0.65.0",
    "flow-typed": "2.3.0",
