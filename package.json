{
  "name": "Vue-Fullstack-Starter",
  "version": "1.0.0",
  "description": "A boilerplate for Vue, Material, and Express.",
  "scripts": {
    "start:app": "webpack-dev-server --progress",
    "start:api": "nodemon src/server.js --exec babel-node",
    "build:app": "webpack --env.prod",
    "build:api": "babel src -d dist --ignore client.js,app",
    "test:app": "eslint src/app && jest \"src/app/.*\\.js$\" --coverage --forceExit",
    "test:api": "eslint src/api && jest \"src/api/.*\\.js$\" --coverage --forceExit",
    "deploy": "firebase deploy",
    "flow": "flow",
    "typed": "flow-typed install",
    "codecov": "codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shyam-Chen/Vue-Fullstack-Starter.git"
  },
  "keywords": [
    "Vue"
  ],
  "author": "Yen-cheng Chen, 陳彥澄",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shyam-Chen/Vue-Fullstack-Starter/issues"
  },
  "homepage": "https://github.com/Shyam-Chen/Vue-Fullstack-Starter#readme",
  "dependencies": {
    "apollo-client": "2.1.0",
    "axios": "0.17.1",
    "cors": "2.8.4",
    "express": "4.16.2",
    "express-history-api-fallback": "2.2.1",
    "rxjs": "5.5.5",
    "vue": "2.5.10",
    "vue-apollo": "3.0.0-alpha.2",
    "vue-i18n": "7.3.2",
    "vue-material": "1.0.0-beta-7",
    "vue-meta": "1.4.0",
    "vue-router": "3.0.1",
    "vue-rx": "5.0.0",
    "vue-server-renderer": "2.5.10",
    "vuex": "3.0.1",
    "vuex-router-sync": "5.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.3",
    "babel-jest": "21.2.0",
    "babel-loader": "7.1.2",
    "babel-plugin-root-import": "5.1.0",
    "babel-plugin-transform-function-bind": "6.22.0",
    "babel-plugin-transform-imports": "1.4.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.1",
    "babel-register": "6.26.0",
    "codecov": "3.0.0",
    "copy-webpack-plugin": "4.2.3",
    "css-loader": "0.28.7",
    "cssnano": "4.0.0-rc.2",
    "eslint": "4.13.1",
    "eslint-plugin-vue": "3.8.0",
    "firebase-tools": "3.16.0",
    "flow-bin": "0.61.0",
    "flow-typed": "2.2.3",
    "html-webpack-plugin": "2.30.1",
    "jest": "21.2.1",
    "jest-vue-preprocessor": "1.3.1",
    "nodemon": "1.12.7",
    "postcss-cssnext": "3.0.2",
    "postcss-import": "11.0.0",
    "postcss-loader": "2.0.9",
    "puppeteer": "0.13.0",
    "rucksack-css": "1.0.2",
    "style-loader": "0.19.0",
    "sw-precache": "5.2.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "uglifyjs-webpack-plugin": "1.1.3",
    "vue-loader": "13.5.0",
    "vue-template-compiler": "2.5.10",
    "vue-test-utils": "1.0.0-beta.7",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.9.7"
  }
}
