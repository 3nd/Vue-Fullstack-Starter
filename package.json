{
  "name": "Vue-Fullstack-Starter",
  "version": "1.0.0",
  "description": "A boilerplate for Vue, Material, and Express.",
  "scripts": {
    "start": "webpack-dev-server --progress",
    "build": "webpack --env.prod",
    "test": "eslint src && jest --coverage --forceExit",
    "lint": "eslint src",
    "unit": "jest --coverage",
    "e2e": "gulp e2e",
    "deploy": "firebase deploy",
    "flow": "flow",
    "typed": "flow-typed install",
    "codecov": "codecov",
    "webdriver": "gulp webdriver"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shyam-Chen/Vue-Fullstack-Starter.git"
  },
  "keywords": [
    "Vue"
  ],
  "author": "Yen-cheng Chen, 陳彥澄",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shyam-Chen/Vue-Fullstack-Starter/issues"
  },
  "homepage": "https://github.com/Shyam-Chen/Vue-Fullstack-Starter#readme",
  "dependencies": {
    "apollo-client": "2.0.3",
    "axios": "0.17.1",
    "express": "4.16.2",
    "express-history-api-fallback": "2.2.1",
    "rxjs": "5.5.2",
    "vue": "2.5.7",
    "vue-apollo": "3.0.0-alpha.2",
    "vue-i18n": "7.3.2",
    "vue-material": "0.8.1",
    "vue-router": "3.0.1",
    "vue-rx": "5.0.0",
    "vue-server-renderer": "2.5.9",
    "vuex": "3.0.1",
    "vuex-router-sync": "5.0.0"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.2",
    "babel-jest": "21.2.0",
    "babel-loader": "7.1.2",
    "babel-plugin-root-import": "5.1.0",
    "babel-plugin-transform-function-bind": "6.22.0",
    "babel-plugin-transform-imports": "1.4.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.1",
    "babel-register": "6.26.0",
    "codecov": "3.0.0",
    "copy-webpack-plugin": "4.2.3",
    "css-loader": "0.28.7",
    "eslint": "4.11.0",
    "eslint-plugin-vue": "3.8.0",
    "firebase-tools": "3.15.2",
    "flow-bin": "0.59.0",
    "flow-typed": "2.2.3",
    "html-webpack-plugin": "2.30.1",
    "jest": "21.2.1",
    "jest-vue-preprocessor": "1.3.1",
    "puppeteer": "0.13.0",
    "style-loader": "0.19.0",
    "sw-precache": "5.2.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "uglifyjs-webpack-plugin": "1.1.1",
    "vue-loader": "13.5.0",
    "vue-template-compiler": "2.5.7",
    "vue-test-utils": "1.0.0-beta.5",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4"
  },
  "jest": {
    "moduleNameMapper": {
      "^vue$": "vue/dist/vue.common.js",
      "~(.*)": "<rootDir>/src/app$1",
      "~/shared(.*)": "<rootDir>/src/app/shared$1"
    },
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue-preprocessor"
    }
  }
}
