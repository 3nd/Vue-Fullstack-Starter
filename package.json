    "postcss-nested": "3.0.0",
    "puppeteer": "1.0.0",
    "rucksack-css": "1.0.2",
    "style-loader": "0.20.1",
    "sw-precache": "5.2.1",
    "sw-precache-webpack-plugin": "0.11.4",
    "uglifyjs-webpack-plugin": "1.1.8",
    "vue-jest": "2.1.0",
    "vue-loader": "14.1.1",
    "vue-template-compiler": "2.5.13",
    "vue-test-utils": "1.0.0-beta.11",
    "webpack": "3.11.0",
    "webpack-dev-server": "2.11.1"
  }
}
